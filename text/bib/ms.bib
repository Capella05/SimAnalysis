@ARTICLE{Coles2008,
  author = {{Coles}, J.},
  title = {{A New Estimate of the Hubble Time with Improved Modeling of Gravitational
	Lenses}},
  journal = {\apj},
  year = {2008},
  volume = {679},
  month = may,
  pages = {17-24},
  doi = {10.1086/587635},
  eprint = {0802.3219},
  abstract = {This paper examines free-form modeling of gravitational lenses using
	Bayesian ensembles of pixelated mass maps. The priors and algorithms
	from previous work are clarified and significant technical improvements
	are made. Lens reconstruction and Hubble time recovery are tested
	using mock data from simple analytic models and recent galaxy formation
	simulations. Finally, using published data, the Hubble time is inferred
	through the simultaneous reconstruction of 11 time delay lenses.
	The result is H-10=13.7+1.8-1.0 Gyr (H0=71+6-8 km s-1 Mpc-1).},
  adsnote = {Provided by the SAO/NASA Astrophysics Data System},
  adsurl = {http://adsabs.harvard.edu/abs/2008ApJ...679...17C},
  archiveprefix = {arXiv},
  file = {:Coles2008.pdf:PDF},
  keywords = {Cosmology: Cosmological Parameters, Cosmology: Gravitational Lensing},
  owner = {RafiK},
  timestamp = {2013.09.01}
}

